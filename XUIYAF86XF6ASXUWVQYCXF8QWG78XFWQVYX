local function ENCRYPTFUNCTION(text, key)
return text:gsub("%a", function(t)
local base = (t:lower() == t and string.byte('a') or string.byte('A'))

local r = t:byte() - base
r = r + key
r = r%26 -- works correctly even if r is negative
r = r + base
return string.char(r)
end)
end

local function DECRYPTFUNCTION(text, key)
return ENCRYPTFUNCTION(text, -key)
end

