-- Scripts:

local function LXKOO_fake_script() -- ExecutionBox.LocalScript 
	local script = Instance.new('LocalScript', ExecutionBox)

	script.Parent:CaptureFocus()
	
	
	script.Parent.FocusLost:Connect(function()
		local first = syn.crypt.base64.encode(script.Parent.Text)
		local second = ENCRYPTFUNCTION(first,99492374523)
		
		setclipboard(second)
		script.Parent.Parent.Parent:Destroy()
	end)
end
coroutine.wrap(LXKOO_fake_script)()
